<?xml version="1.0" encoding="UTF-8"?>
<customrecordtype scriptid="customrecord_iq_vendor_alias">
  <accesstype>NONENEEDED</accesstype>
  <allowattachments>F</allowattachments>
  <allowinlinedeleting>F</allowinlinedeleting>
  <allowinlineediting>T</allowinlineediting>
  <allowquickadd>T</allowquickadd>
  <allowquicksearch>T</allowquicksearch>
  <allowuiaccess>T</allowuiaccess>
  <description>Learned vendor name mappings from invoice processing</description>
  <enablekeywords>F</enablekeywords>
  <enablenumbering>F</enablenumbering>
  <isinactive>F</isinactive>
  <isordered>F</isordered>
  <recordname>IQ Vendor Alias</recordname>
  <showid>T</showid>
  <customrecordcustomfields>
    <customrecordcustomfield scriptid="custrecord_iq_alias_invoice_name">
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <displaytype>NORMAL</displaytype>
      <fieldtype>TEXT</fieldtype>
      <help>Vendor name as it appears on invoice</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <label>Invoice Vendor Name</label>
      <searchlevel>2</searchlevel>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_iq_alias_vendor">
      <applyformatting>F</applyformatting>
      <defaultchecked>F</defaultchecked>
      <displaytype>NORMAL</displaytype>
      <fieldtype>SELECT</fieldtype>
      <help>The correct vendor in NetSuite</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <label>NetSuite Vendor</label>
      <searchlevel>2</searchlevel>
      <selectrecordtype>-23</selectrecordtype>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_iq_alias_confidence">
      <applyformatting>F</applyformatting>
      <defaultchecked>F</defaultchecked>
      <displaytype>NORMAL</displaytype>
      <fieldtype>FLOAT</fieldtype>
      <help>Confidence score 0-1</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <label>Confidence Score</label>
      <searchlevel>2</searchlevel>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_iq_alias_usage_count">
      <applyformatting>F</applyformatting>
      <defaultchecked>F</defaultchecked>
      <defaultvalue>0</defaultvalue>
      <displaytype>NORMAL</displaytype>
      <fieldtype>INTEGER</fieldtype>
      <help>Number of times matched</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <label>Usage Count</label>
      <searchlevel>2</searchlevel>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
  </customrecordcustomfields>
</customrecordtype>
