<?xml version="1.0" encoding="UTF-8"?>
<customrecordtype scriptid="customrecord_iq_vendor_cache">
  <accesstype>NONENEEDED</accesstype>
  <allowattachments>F</allowattachments>
  <allowinlinedeleting>F</allowinlinedeleting>
  <allowinlineediting>F</allowinlineediting>
  <allowquickadd>F</allowquickadd>
  <allowquicksearch>T</allowquicksearch>
  <allowuiaccess>T</allowuiaccess>
  <description>Cache for vendor list to optimize governance usage</description>
  <enablekeywords>F</enablekeywords>
  <enablenumbering>F</enablenumbering>
  <isinactive>F</isinactive>
  <isordered>F</isordered>
  <recordname>IQ Vendor Cache</recordname>
  <showid>T</showid>
  <customrecordcustomfields>
    <customrecordcustomfield scriptid="custrecord_iq_cache_data">
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <displaytype>NORMAL</displaytype>
      <fieldtype>TEXTAREA</fieldtype>
      <help>JSON string containing vendor data</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <label>Cache Data</label>
      <searchlevel>2</searchlevel>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_iq_cache_updated">
      <applyformatting>T</applyformatting>
      <defaultchecked>F</defaultchecked>
      <displaytype>NORMAL</displaytype>
      <fieldtype>DATETIMETZ</fieldtype>
      <help>Timestamp when cache was last updated</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <label>Last Updated</label>
      <searchlevel>2</searchlevel>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_iq_cache_record_count">
      <applyformatting>F</applyformatting>
      <defaultchecked>F</defaultchecked>
      <displaytype>NORMAL</displaytype>
      <fieldtype>INTEGER</fieldtype>
      <help>Number of vendor records in cache</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <label>Vendor Count</label>
      <searchlevel>2</searchlevel>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
  </customrecordcustomfields>
</customrecordtype>
